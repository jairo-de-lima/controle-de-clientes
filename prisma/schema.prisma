generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql" // Ajuste para o banco que est√° usando
  url      = env("DATABASE_URL")
}

model Cliente {
  id                 String   @id @default(uuid())
  cliente            String
  endereco           String
  documento          BigInt
  fornecedor         String
  transportadora     String
  telefoneResidencial BigInt
  celular            BigInt
  suframa            String
  email              String   @unique
  createdAt          DateTime @default(now())
  updatedAt          DateTime @updatedAt

  @@map("clientes")
}
